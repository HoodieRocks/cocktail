{
  "schemaVersion": 1,
  "id": "cocktail",
  "version": "${version}",
  "name": "Cocktail",
  "description": "A mod to extend the power of Datapacks",
  "authors": [
    "Cbble_",
    "Datapack-Hub"
  ],
  "contact": {
    "repo": "https://github.com/Datapack-Hub/Cocktail"
  },
  "license": "GPL-3.0",
  "icon": "assets/cocktail/icon.png",
  "environment": "*",
  "entrypoints": {
    "server": [
      {
        "adapter": "kotlin",
        "value": "me.cobble.cocktail.server.CocktailServer"
      }
    ],
    "main": [
      {
        "adapter": "kotlin",
        "value": "me.cobble.cocktail.Cocktail"
      }
    ],
    "client": [
      {
        "adapter": "kotlin",
        "value": "me.cobble.cocktail.client.CocktailClient"
      }
    ]
  },
  "mixins": [
    "cocktail.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric": "*",
    "minecraft": "${minecraft_version}",
    "fabric-language-kotlin": ">=1.10.19+kotlin.1.9.23"
  }
}
